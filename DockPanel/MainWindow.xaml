<Window x:Class="PracticaStackPanel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaStackPanel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Orientation="Vertical">
        <Button VerticalAlignment="Stretch">Botón A</Button>
        <Button VerticalAlignment="Top">Botón B</Button>
        <Button VerticalAlignment="Center">Botón C</Button>
        <Button VerticalAlignment="Bottom">Botón D</Button>
        <Button HorizontalAlignment="Stretch">Botón E</Button>
        <Button HorizontalAlignment="Left">Botón F</Button>
        <Button HorizontalAlignment="Center">Botón G</Button>
        <Button HorizontalAlignment="Right" Width="150" HorizontalContentAlignment="Left">Botón H</Button>
        <Button Width="250">Botón I</Button>
        <Button>Botón J</Button>
        <Button>Botón K</Button>
        <Button>Botón L</Button>
    </StackPanel>
</Window>
